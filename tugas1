#include <iostream>

using namespace std;
 
int asc;
 
int main(){
    
    int nilai[5];
 
    for(int i=0; i<5; i++){
        cout << "Nilai " << (i+1) << " : ";
        cin >> nilai[i];
    }
 
    for(int c=1;c<5;c++)
    { 
        for(int d=0;d<5-c;d++)
        { 
            if(nilai[d]<nilai[d+1])
            { 
                asc=nilai[d];
                nilai[d]=nilai[d+1];
                nilai[d+1]=asc; 
            } 
        } 
    }
 
    cout<<"\nHasil Pengurutan Adalah ";
    for(int i=0;i<5;i++)
    {
        cout<<"\n"<<nilai[i]<<endl;
    } 
    return 0;
}
